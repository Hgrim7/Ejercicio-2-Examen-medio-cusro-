# Ejercicio-2-Examen-medio-cusro-
//Inciso a)
size(100,100);
background(255);
fill(0);
rectMode(CENTER);
rect(50,50,90,90);
fill(255);
ellipse(50,50,30,30);

//Inciso b)
size(100,100);
background(255);
fill(0);
int x = 50;
int y = 90;
int c = 30;
rectMode(CENTER);
rect(x,x,y,y);
fill(255);
ellipse(x,x,c,c);
